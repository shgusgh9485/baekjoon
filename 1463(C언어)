#include <stdio.h>
#define min2(a, b) a<b ? a : b

int main() {
  int N;
  int x[1000001]={0,}; //정수 자체를 인덱스로 사용하기에(x[0]은 사용하지 않고 x[1]은 정수 1의 최소 횟수)  1000000이 아닌 1000001으로 배열 범위 설정.
  int min_num;

  scanf("%d", &N);

  for(int i=2; i<=N; ++i){ //N까지 최소 횟수를 초기화 해줌.
    min_num=x[i-1]+1; //3번 조건인 1을 뺐을 경우의 최소 횟수.
    if(i%3==0) min_num=min2(min_num, x[i/3]+1); //3으로 나누어 떨어지는 1번 조건과 3번 조건 중 최소 횟수가 더 적은 것을 선택함.
    if(i%2==0) min_num=min2(min_num, x[i/2]+1); //2로 나누어 떨어지는 2번 조건과 3번 조건 중 최소 횟수가 더 적은 것을 선택함.
    x[i]=min_num;
  } //+1을 붙힌 것은 나누기를 시도하든 빼기를 시도하든 그 횟수가 1이기에 +1을 한 것임.

  printf("%d", x[N]); //N의 최소 횟수 출력.
  
  return 0;
}
